public class InformacoesPessoais {
    public static void main(String[] args) {
        // Declaração e inicialização das variáveis
        String nome = "Paulo";  // Exemplo de nome
        int idade = 29;  // Exemplo de idade
        int batimentosCardiacos = 220 - idade;  // Cálculo dos batimentos cardíacos
        
        // Imprimir as informações no console
        System.out.println("Nome: " + nome);
        System.out.println("Idade: " + idade);
        System.out.println("Batimentos Cardíacos: " + batimentosCardiacos);
    }
}